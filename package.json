{
  "name": "aip-pdf-viewer",
  "private": true,
  "packageManager": "pnpm@9.15.4",
  "scripts": {
    "dev": "node scripts/dev.mjs",
    "dev:split": "pnpm -r dev",
    "build": "pnpm -r build",
    "start": "pnpm --filter @aip/server start",
    "dist:win": "pnpm --filter @aip/launcher dist:win",
    "docker:build": "docker build -t eaip-pdf-viewer:local .",
    "docker:redeploy": "node scripts/docker-redeploy.mjs",
    "typecheck": "pnpm -r typecheck"
  }
}


